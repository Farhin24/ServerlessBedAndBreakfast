;\r\n      })}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Notifications;\r\n","C:\\Users\\hirva\\Documents\\Dal\\Summer Term\\CSCI 5410\\Project\\Project Code\\csci5410_group30\\client\\src\\booking\\Tour.js",[],"C:\\Users\\hirva\\Documents\\Dal\\Summer Term\\CSCI 5410\\Project\\Project Code\\csci5410_group30\\client\\src\\user\\Dashboard.js",["129","130"],"// import DashboardNav from \"../components/DashboardNav\";\r\n// import ConnectNav from \"../components/ConnectNav\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { userHotelBookings } from \"../actions/hotel\";\r\nimport { useSelector } from \"react-redux\";\r\nimport { useState, useEffect } from \"react\";\r\n\r\nconst Dashboard = () => {\r\n  const {\r\n    auth: { token },\r\n  } = useSelector((state) => ({ ...state }));\r\n  const [booking, setBooking] = useState([]);\r\n\r\n  useEffect(() => {\r\n    loadUserBookings();\r\n  }, []);\r\n\r\n  const loadUserBookings = async () => {\r\n    const res = await userHotelBookings(token);\r\n    console.log(res);\r\n    setBooking(res.data);\r\n  };\r\n\r\n  return (\r\n    <>\r\n      {/* <div className=\"container-fluid bg-secondary p-5\">\r\n        <ConnectNav />\r\n      </div>\r\n\r\n      <div className=\"container-fluid p-4\">\r\n        <DashboardNav />\r\n      </div> */}\r\n\r\n      <div className=\"container-fluid\">\r\n        <div className=\"row\">\r\n          <div className=\"col-md-10\">\r\n            <h2>Your Bookings</h2>\r\n          </div>\r\n          <div className=\"col-md-2\">\r\n            <Link to=\"/\" className=\"btn btn-primary\">\r\n              Browse Hotels\r\n            </Link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div className=\"row\"></div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Dashboard;\r\n","C:\\Users\\hirva\\Documents\\Dal\\Summer Term\\CSCI 5410\\Project\\Project Code\\csci5410_group30\\client\\src\\auth\\Cipher.js",["131"],"import React, { useEffect, useState }